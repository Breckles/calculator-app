  "version": "0.2.0",
  "configurations": [{
      "name": "FireFox React",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "localhost:3000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Chrome React",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    },
  ]
}